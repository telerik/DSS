    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "mocha": "^8.2.1",
    "semantic-release": "^17.3.9"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
