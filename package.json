{
  "name": "@progress/dss",
  "description": "Documented Style Sheets",
  "version": "1.0.0",
  "homepage": "https://github.com/telerik/dss",
  "author": "Progress",
  "license": "MIT",
  "keywords": [
    "css",
    "dss",
    "styles",
